class Solution {
    public int lengthOfLongestSubstring(String s) {
        Set<Character> ch = new HashSet<>();
        int max =0;
        int left =0;
        for(int r=0;r<s.length();r++){
            while(ch.contains(s.charAt(r))){
                ch.remove(s.charAt(left));
                left++;
            }
            ch.add(s.charAt(r));
            max = Math.max(max,r-left+1);
        }  
        return max;
    }      
}
