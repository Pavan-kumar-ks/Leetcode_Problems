class Solution {
    public int numOfSubarrays(int[] arr, int k, int t) {
        int sum=0;
        for(int i=0;i<k;i++){
            sum += arr[i];
        }
        int c=0;
        if(sum>=k*t){
            c++;
        }
        for(int i=k;i<arr.length;i++){
            sum -= arr[i-k];
            sum += arr[i];
            if(sum>=k*t){
                c++;
            }
        }
        return c;
    }
}
