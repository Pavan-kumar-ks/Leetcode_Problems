class Solution {
    public int averageValue(int[] nums) {
        int res=0;
        int c=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]%3 == 0&& nums[i]%2==0){
                    res += nums[i];
                    c++;
            }
        }
        return (c==0)?0:res/c;
    }
}
