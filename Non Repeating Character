class Solution {
    static char nonRepeatingChar(String s) {
        // code here
        char[] ch = s.toCharArray();
        HashMap<Integer,Integer> map = new HashMap<>();
        int val =0;
        for(int i=0;i<ch.length;i++){
            map.put(ch[i],map.getOrDefault(ch[i],0)+1);
            if(!map.containsKey(ch[i])){
                val = map.get()
            }
        }
    }
}
